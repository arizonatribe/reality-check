{
  "name": "reality-check",
  "description": "A unit-test like wrapper around benchmark.js, for testing CPU performance of functions in JavaScript or TypeScript",
  "main": "lib/index.js",
  "version": "1.0.0",
  "license": "MIT",
  "types": "index.d.ts",
  "author": "David Nunez <arizonatribe@gmail.com>",
  "engines": {
    "node": ">=7.6.0"
  },
  "bin": {
    "rcheck": "./bin/index.js",
    "reality-check": "./bin/index.js",
    "benchmark-check": "./bin/index.js"
  },
  "homepage": "https://github.com/arizonatribe/reality-check",
  "repository": {
    "type": "git",
    "url": "https://github.com/arizonatribe/reality-check.git"
  },
  "keywords": [
    "reality check",
    "benchmark",
    "benchmarks",
    "jsbenchmark",
    "jsperf",
    "testing",
    "benchmarking",
    "performance",
    "typescript",
    "javascript"
  ],
  "scripts": {
    "start": "node bin/index.js",
    "format": "eslint --fix bin lib",
    "benchmark-class": "node --expose-gc test/classMemory.test.js",
    "lint": "eslint bin lib"
  },
  "dependencies": {
    "@vanillas/chalk-console-logger": "^1.0.0",
    "@vanillas/cli-toolkit": "^1.7.0",
    "benchmark": "2.1.4",
    "vanillas": "1.6.5"
  },
  "devDependencies": {
    "@vanillas/eslint-config": "^1.1.0",
    "eslint": "^8.12.0",
    "typescript": "~4.5.2"
  }
}
